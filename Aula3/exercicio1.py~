import math
#Exercicio1
def y(arg):
    print(arg,type(arg))

y(2)

#Exercicio2
def v(t):
    print(s/t)

s=2 #distancia

v(2)

def v(t):
    print(g*t)

g=10 #aceleração(m/s²)

v(2)

#Exercico3
def theta(h,d):
    x=h/d
    print(x)
    print(math.atan(x))
    return x

theta(5,0.5)

#Exercicio4
def metros(milha):
    y=milha/1609.34
    print(y)
    
metros(1)

def milha(metros):
    x=metros*1609.34
    print(x)

milha(1)

def horas(minutos):
    y=minutos/60
    print(y)
horas(30)
    








